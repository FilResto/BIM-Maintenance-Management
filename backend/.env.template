# Scripts Environment Variables Template  
# Copy this file to .env and replace the placeholder values with your actual values

# Network RPC URLs
POLYGON_AMOY_RPC_URL="https://polygon-amoy.infura.io/v3/YOUR_INFURA_API_KEY"
SEPOLIA_RPC_URL="https://sepolia.infura.io/v3/YOUR_INFURA_API_KEY"

# Private Keys for deployment (use test keys only)
